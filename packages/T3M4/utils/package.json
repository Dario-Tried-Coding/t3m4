{
  "name": "@t3m4/utils",
  "version": "0.0.1",
  "description": "Typescript utilities for @T3M4 ecosystem.",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "test": "vitest run",
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint .",
    "check-types": "tsc --noEmit"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./nullables": {
      "import": "./dist/nullables.mjs",
      "types": "./dist/nullables.d.ts",
      "default": "./dist/nullables.js"
    },
    "./objects": {
      "import": "./dist/objects.mjs",
      "types": "./dist/objects.d.ts",
      "default": "./dist/objects.js"
    }
  },
  "keywords": [
    "typescript",
    "utilities"
  ],
  "author": "dariotriedcoding",
  "repository": {
    "type": "git",
    "url": "https://github.com/Dario-Tried-Coding/T3M4"
  },
  "license": "MIT",
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@repo/typescript-config": "workspace:*",
    "@typescript-eslint/eslint-plugin": "^8.24.1",
    "@typescript-eslint/parser": "^8.24.1",
    "eslint": "^9.21.0",
    "tsup": "^8.3.6",
    "typescript": "^5.7.3",
    "vitest": "^3.0.7"
  }
}