{
  "name": "@t3m4/react",
  "version": "0.0.1",
  "description": "A typesafe abstraction for advanced theming in your React apps.",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "test": "vitest run",
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "tsc"
  },
  "keywords": [
    "theming",
    "typescript",
    "react",
    "next.js"
  ],
  "author": "dariotriedcoding",
  "repository": {
    "type": "git",
    "url": "https://github.com/Dario-Tried-Coding/next-themes"
  },
  "license": "MIT",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./types": {
      "import": "./dist/types.mjs",
      "types": "./dist/types.d.ts",
      "default": "./dist/types.js"
    }
  },
  "dependencies": {
    "@repo/typescript-utils": "workspace:*",
    "@t3m4/core": "workspace:*"
  },
  "peerDependencies": {
    "react": "^16.8 || ^17 || ^18 || ^19 || ^19.0.0-rc"
  },
  "devDependencies": {
    "@repo/typescript-config": "workspace:*",
    "@types/react": "19.0.10",
    "react": "^19.0.0",
    "tsup": "^8.3.5",
    "typescript": "^5.7.3",
    "vitest": "^3.0.4"
  }
}